<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">

	<!-- frontend service -->
	<bean id="frontendMicroService" class="eu.comparegroup.configuration.MicroService">
		<property name="name" value="${frontendService.name}" />
		<property name="protocol" value="${frontendService.protocol}" />
		<property name="server" value="${frontendService.server}" />
		<property name="port" value="${frontendService.port}" />
		<property name="adminPort" value="${frontendService.adminPort}" />
		<property name="endPoint" value="${frontendService.endPoint}" />
	</bean>

</beans>
